alias cls='clear'
alias vim='nvim'
alias ss='source ~/.zshrc'
alias oh='cd ~'

alias be='cd ~/MyProjects/BE'
alias fe='cd ~/MyProjects/FE'
alias myproject='cd ~/MyProjects'
alias vimconfig='cd ~/dotfiles/nvim/.config/nvim'
alias myalias='cd ~/dotfiles/zsh/.oh-my-zsh/custom'
alias weztermconfig='cd ~/dotfiles/wezterm/.config/wezterm/'

alias mongostart='mongod --config /opt/homebrew/etc/mongod.conf --fork --logpath /opt/homebrew/var/log/mongodb/mongo.log'
alias mongostop='mongod --shutdown --config /opt/homebrew/etc/mongod.conf'
